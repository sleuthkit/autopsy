      <documentation xml:lang="en" source="http://www.w3.org/TR/xmlschema-2/#Name"/>
    </annotation>
    <restriction base="token">
      <pattern value="\i\c*" id="Name.pattern">
        <annotation>
          <documentation xml:lang="en" source="http://www.w3.org/TR/REC-xml#NT-Name">
